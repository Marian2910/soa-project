{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "JwtSettings": {
    "Key": "YOUR_JWT_SECRET_HERE",
    "Issuer": "MfaAuthService",
    "Audience": "MfaClients"
  },

  "MongoDb": {
    "ConnectionString": "mongodb://<username>:<password>@<host>:<port>"
  },

  "RabbitMQ": {
    "HostName": "<hostname>>",
    "UserName": "<username>>",
    "Password": "<password>>"
  },

  "Kafka": {
    "BootstrapServers": "127.0.0.1:9092"
  }
}
